      "bundleIdentifier": "com.foodsavers.FoodSmart"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.foodsavers.FoodSmart"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c4719ca3-29c1-4816-9e18-03ceab1e1ac5"
      }
    },
    "owner": "sonpapadi"
  }
}
